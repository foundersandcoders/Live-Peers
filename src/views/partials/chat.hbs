<div class="chat">
  <!-- Chat Window -->
  <div class="paneselect chat__window">
    <div class="chat__title-container">
      <h3 class="chat__title">Chat</h3>
    </div>
    <div class="chat__messages">
      <!-- username:message -->
    </div>
    <form class="chat__form">
      <input type="text" class="chat__input" placeholder="Type a message">
      <button type="submit" class="chat__send" value="send">Send</button>
    </form>
  </div>
  <!-- Online Window -->
  <div class="online__window">
    <div class="online__title-container">
      <h3 class="online__title">Online</h3>
    </div>
    <div class="online__mentor">
      <h1 class="online__header">Mentor</h1>
      <ul class="online__mentor-list">
        <!-- Mentor -->
      </ul>
    </div>
    <div class="online__moderator">
      <h1 class="online__header">Moderator</h1>
      <ul class="online__moderator-list">
        <!-- Moderator -->
      </ul>
    </div>
    <div class="online__students">
      <h1 class="online__header">Students</h1>
      <ul class="online__students-list">
        <!-- Students -->
      </ul>
    </div>
  </div>
</div>
